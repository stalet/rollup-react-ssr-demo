{
  "name": "rollup-hello-world",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist && mkdir dist",
    "build": "npm run build:js && npm run build:css && npm run build:icons",
    "build:js": "rollup -c",
    "build:icons": "cp client/icons/* dist",
    "build:css": "postcss ./client/style.css > dist/output.css ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --es-module-specifier-resolution=node .",
    "start:dev": "npm run build && npm run start"
  },
  "cacheDirectories": [
    "node_modules",
    "dist"
  ],
  "author": "stalet",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "@babel/core": "7.25.9",
    "@babel/preset-env": "7.25.9",
    "@babel/preset-react": "7.25.9",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "25.0.8",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@rollup/plugin-replace": "5.0.7",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.47",
    "postcss-cli": "11.0.0",
    "rimraf": "5.0.10",
    "rollup": "4.14.0",
    "@rollup/plugin-babel": "6.0.4",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-serve": "3.0.0",
    "rollup-plugin-uglify": "4.0.0",
    "tailwindcss": "3.4.14"
  }
}
