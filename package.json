{
  "name": "rollup-hello-world",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist && mkdir dist",
    "build": "npm run build:js && npm run build:css && npm run build:icons",
    "build:js": "rollup -c",
    "build:icons": "cp client/icons/* dist",
    "build:css": "postcss ./client/style.css > dist/output.css ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --es-module-specifier-resolution=node .",
    "start:dev": "npm run build && npm run start"
  },
  "cacheDirectories": [
    "node_modules",
    "dist"
  ],
  "author": "stalet",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "@babel/core": "7.22.11",
    "@babel/preset-env": "7.22.10",
    "@babel/preset-react": "7.22.5",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "25.0.4",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-replace": "5.0.2",
    "autoprefixer": "10.4.15",
    "postcss": "8.4.28",
    "postcss-cli": "10.1.0",
    "rimraf": "5.0.1",
    "rollup": "2.79.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-serve": "2.0.2",
    "rollup-plugin-uglify": "4.0.0",
    "tailwindcss": "3.3.3"
  }
}
